{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "double"],
        "semicolon": [true, "never"],
        "indent": [true, "spaces", 2],
        "eofline": true,
        "interface-name": false,
        "member-access": false,
        "unified-signatures": false,
        "ban-types": false,
        "only-arrow-functions": false,
        "object-literal-sort-keys": false,
    },
    "rulesDirectory": []
}
